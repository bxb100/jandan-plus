[
    {
        "id": 1,
        "priority": 1,
        "action": {
            "type": "redirect",
            "redirect": {
                "regexSubstitution": "https://\\1.sinaimg.cn/\\2"
            }
        },
        "condition": {
            "regexFilter": "^https:\/\/(.*)\\.moyu\\.im\/(.*)",
            "resourceTypes": [
                "main_frame",
                "xmlhttprequest",
                "image"
            ]
        }
    },
    {
        "id": 2,
        "priority": 1,
        "action": {
